{
  "InstrBlock": {
    "prefix": "di",
    "body": "{#${1:instr}]$0\n[#}"
  },
  "InstrBlockNamed": {
    "prefix": "dii",
    "body": "{#${1:instr}]$0\n[#${1:instr}}"
  },
  "InstrBlock1": {
    "prefix": "di1",
    "body": "{#${1:instr}]$0[#}"
  },
  "InstrBlock1Named": {
    "prefix": "dii1",
    "body": "{#${1:instr}]$0[#${1:instr}}"
  },
  "InstrBeg": {
    "prefix": "dib",
    "body": "{#${1:instr}]$0"
  },
  "InstrEnd": {
    "prefix": "die",
    "body": "[#${1:end}}$0"
  },
  "Tag": {
    "prefix": "dt",
    "body": "[#${1:instr}]$0",
    "descrioption": "sequential [#tag]"
  },
  "Var": {
    "prefix": "dvar",
    "body": "[#=]${1:varname}[${2:value}]$0"
  },
  "Case": {
    "prefix": "dcase",
    "body": "{#case]${1:choices}\n[#when]${2:case}[#${2:instr}]$0\n[#}"
  },
  "CaseOneLine": {
    "prefix": "dcase1",
    "body": "{#case]${1:choices}[#when]${2:case}[#${2:instr}]$0[#case}"
  },
  "When": {
    "prefix": "dwhen",
    "body": "[#when]${1:case}[#${2:instr}]$0"
  }
}